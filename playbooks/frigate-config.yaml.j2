version: 0.14
mqtt:
  enabled: false

tls:
  enabled: false

auth:
  enabled: false

proxy:
  header_map:
    user: x-forwarded-user

record:
  enabled: True
  alerts:
    retain:
      days: 14
      mode: active_objects

motion:
  frame_height: 25
  threshold: 20

detect:
  enabled: true
  width: 640
  height: 360
  fps: 1

detectors:
  ov:
    type: openvino
    device: CPU

model:
  width: 300
  height: 300
  input_tensor: nhwc
  input_pixel_format: bgr
  path: /openvino-model/ssdlite_mobilenet_v2.xml
  labelmap_path: /openvino-model/coco_91cl_bkgr.txt

review:
  alerts:
    labels: []

cameras:
  Vent:
    ffmpeg:
      inputs:
        - path: rtsp://admin:thelab.ms@10.200.2.17:554/cam/realmonitor?channel=1&subtype=0

  Storage:
    ffmpeg:
      inputs:
        - path: rtsp://admin:thelab.ms@10.200.2.6:554/cam/realmonitor?channel=1&subtype=0

  Atrium:
    ffmpeg:
      inputs:
        - path: rtsp://admin:thelab.ms@10.200.2.13:554/cam/realmonitor?channel=1&subtype=0

  Woodshop:
    ffmpeg:
      inputs:
        - path: rtsp://admin:thelab.ms@10.200.2.18:554/cam/realmonitor?channel=1&subtype=0

  Crafting:
    ffmpeg:
      inputs:
        - path: rtsp://10.200.2.16:554/Streaming/Channels/101

